command /give [<player>] [<text>] [<integer>]:
	permission: proton.give
	permission message: {@em}
	aliases: /giveitem
	usage: /give [player|*] [item] (amount)
	description: Give a player an item
	aliases: /g
	trigger:
		if arg 1 is set:
			if arg 2 is set:
				set {_item} to "%arg-2%"
				replace all "_" with " " in {_item}
				set {_item} to "%{_item}%" parsed as item
				if {_item} is an item:
					if arg 3 is set:
						set {_amount} to arg-3
						loop {_amount} times:
							give {_item} to player-arg
						if sender is player-arg:
							send "{@p} Recieved &a%{_amount}%x %{_item}%{@t}."
						else:
							send "{@p} Given &a%{_amount}%x %{_item}%{@t} to {@h}%player-arg%{@t}."
					else:
						give {_item} to player-arg
						if sender is not player-arg:
							send "{@p} Recieved &a1x %{_item}%{@t}."
						else:
							send "{@p} Given &a1x %{_item}%{@t} to {@h}%player-arg%{@t}."
				else:
					send "{@a} &6%arg-2%&c is not an item."
					send "&cUsage: /give %arg-1% &m%arg-2%&r&c (amount)"
			else:
				send "&cUsage: /give %arg-1% [item] (amount)"
		else:
			send "&cUsage: /give [player] [item] (amount)"
command /item [<text>]:
	permission: proton.item
	permission message: {@em}
	usage: /item [item] (amount)
	aliases: /i
	description: Gives an item to sender
	trigger:
		if arg 1 is set:
			player command "/give %sender% %arg 1%"
